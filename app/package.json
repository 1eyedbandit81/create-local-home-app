{
  "private": true,
  "dependencies": {
    "@google/local-home-sdk": "^0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "ava": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "parcel": "^1.12.4",
    "rollup": "^1.20.0",
    "rollup-plugin-copy": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-serve": "^1.0.1",
    "shx": "^0.3.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "test": "ava",
    "lint": "tslint --project .",
    "build-none": "tsc && shx cp index.none.html dist/index.html",
    "build-webpack": "webpack",
    "build-rollup": "rollup -c",
    "build-parcel": "parcel build index.parcel.html --bundle-node-modules --out-file index.html",
    "start-none": "npx serve dist/",
    "start-webpack": "webpack-dev-server",
    "start-rollup": "rollup -c rollup.serve.js",
    "start-parcel": "parcel serve index.parcel.html -o index.html"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
