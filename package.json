{
  "name": "@google/create-local-home-app",
  "version": "0.1.1",
  "description": "Create Local Home SDK apps.",
  "bin": "./main.js",
  "author": "Google",
  "license": "Apache-2.0",
  "repository": "actions-on-google/create-local-home-app",
  "homepage": "https://github.com/actions-on-google/create-local-home-app#readme",
  "bugs": {
    "url": "https://github.com/actions-on-google/create-local-home-app/issues"
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "tsc",
    "prepare" : "npm run build",
    "test": "ava test.ts",
    "lint": "tslint --project .",
    "start": "ts-node main.ts"
  },
  "dependencies": {
    "array.prototype.flatmap": "^1.2.3",
    "replace-in-file": "^4.3.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@types/array.prototype.flatmap": "^1.2.0",
    "@types/node": "^12.12.26",
    "@types/yargs": "^13.0.7",
    "ava": "^3.4.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "index.js",
    "main.js",
    "npm-shrinkwrap.json",
    "app/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
